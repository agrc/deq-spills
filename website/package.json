  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "pnpm start",
    "format": "prettier . --write",

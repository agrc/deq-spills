    "cpy-cli": "^6.0.0",
    "eslint": "^9.39.2",
    "happy-dom": "^20.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.22",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.19",
    "tailwindcss-react-aria-components": "^1.2.0",

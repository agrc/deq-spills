    "@ugrc/utilities": "^3.1.1",
    "firebase": "^12.8.0",
    "ky": "^1.14.3",
    "lodash.startcase": "^4.4.0",
    "react": "^19.2.4",
    "react-aria-components": "^1.14.0",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",

    "@ugrc/utilities": "^3.1.0",
    "firebase": "^12.2.0",
    "ky": "^1.8.2",
    "lodash.startcase": "^4.4.0",
    "react": "^19.1.1",
    "react-aria-components": "^1.11.0",
    "react-dom": "^19.1.1",
